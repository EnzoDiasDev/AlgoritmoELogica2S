Algoritmo "semnome"
//Solicitar dois valores distintos e informar qual é o maior.
Var
   n1, n2: real
Inicio
      Escreva("Digite o primeiro número: ")
      Leia(n1)
      Escreva("Digite o segundo número: ")
      Leia(n2)
      
      Se (n1 > n2) entao
         Escreva("O número", n1, " é o maior!")
      Senao
           Escreva("O número", n2, " é o maior!")
      FimSe

Fimalgoritmo