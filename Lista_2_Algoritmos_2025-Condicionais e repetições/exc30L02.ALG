Algoritmo "semnome"
// Apresentar todos os números ímpares entre 5 e 50, um ao lado do outro
// (utilizando PARA).

Var
   n, i: inteiro


Inicio
      n <- 5
      Para i <- 5 ate 27 faca
           Se (n = 49) entao
              Escreva(n)
           Senao
                Escreva(n, ", ")
           FimSe

           n <- n + 2
      FimPara

Fimalgoritmo